#import compile as c
#import io
#import init

puts :: fn #foreign(str: *u8);

compile :: fn() -> c.CompileInfo {
    puts(c"c string");
    out := c.CompileInfo {
        files = []string { "../file1.rcp", "../file2.rcp" },
        opt = 1,
        flags = @u32 (c.CompileFlag.SanAddress),
    };

   for f in out.files {
       io.println("Compiling file: %", f);
   }

    return out;
}

