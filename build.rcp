
memset :: fn(dst: *, val: i32, setsize: int) -> *;
malloc :: fn(size: int) -> *;

read_stdin_line :: fn(buff: *, max: i32, stream: *) -> cstring;
__acrt_iob_func :: fn(_Ix: u32) -> *;

compile :: fn(out: *CompileInfo) {
	to_fill := *out;
	for i := 0; i < 5; i += 1 {
		buff := malloc(32);
		memset(buff, 0, 32);
		to_fill.files[i] = read_stdin_line(buff, 32, __acrt_iob_func(0));
	}
	to_fill.file_count = 5;
	*out = to_fill;
}

