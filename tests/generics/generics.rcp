module main

n :: fn(T: type, x: T, y: T) -> T {
	return x + y;
}

get_num :: fn(T: type, m: T) -> T {
    x: T = 10;
    y: T = 20;
    return n(T, x, y) * m;
}

get_other_num :: fn(m: $T) -> T {
    return m + 3;
}

main :: fn() -> i32 {
    foo := get_num(int, 3);
    bar := get_num(i32, 1);
	return foo as i32 - (bar * get_other_num(0 as i32));
}
