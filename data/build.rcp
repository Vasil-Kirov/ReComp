#import compile as c
#import mem
#import array as arr

compile :: fn() -> c.CompileInfo {
    alloc := mem.create_generic_allocator();
    out := c.CompileInfo {
        files = arr.create(string, &alloc),
        opt = 0,
    };

    arr.add(&out.files, "../file1.rcp");
    arr.add(&out.files, "../file2.rcp");

    return out;
}

