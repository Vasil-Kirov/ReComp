#import compile as c
#import io
#import init

puts :: fn #foreign(str: *u8);

compile :: fn() -> c.CompileInfo {
    out := c.CompileInfo {
        files = []string { "../../file1.rcp", "../../file2.rcp" },
        opt = 0,
        flags = @u32 (c.CompileFlag.NoLink | c.CompileFlag.CrossAndroid | c.CompileFlag.NoStdLib),
    };

   for f in out.files {
       io.println("Compiling file: %", f);
   }

    return out;
}

