main

#import intrin as *

struct SomeVals {
	a: f32,
	b: f32,
}

get_struct :: fn(val: int) -> SomeVals {
	return SomeVals { @f32 val * 2, @f32 val };
}

get_array :: fn(x: SomeVals) -> [10]i32 {
    debug_break();
	return [10]i32 { 1, 2, @i32 x.a, @i32 x.b, 5 };
}

main :: fn() -> i32 {
	arr := #run get_array( #run get_struct(10) );
	sum := @i32 0;

	for it in arr	sum += it;

    return sum - 38;
}

